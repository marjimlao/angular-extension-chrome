{
  "manifest_version": 2,
  "name": "Angular: Desarrollando extensiones para Chrome",
  "description": "Consulta de estaciones de Sevici",
  "version": "1.0",
  "browser_action": {
    "default_icon": "assets/icons/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*"
  ]
}
